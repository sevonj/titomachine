;
; test_arithmetic.k91
;
; This one does arithmetic operations.
; When this halts, R2 should contain 1.
;

; 2+2=4
load r1, =2;
add r1, =2;
out r1, =crt;

; 3+4=7
load r2, =3;
add r2, r1;
out r2, =crt;

; 7-2=5
sub r2, =2;
out r2, =crt;

; 5-4=1
sub r2, r1;
out r2, =crt;

; 1*3=3
mul r2, =3;
out r2, =crt;

; 3*-2=-6
load r3, =-2;
mul r2, r3;
out r2, =crt;

; -6/2=-3
div r2, =2;
out r2, =crt;

; -3/-3=1
div r2, =-3;
out r2, =crt;

hcf;

