; Sample program:
; Fill a table with 5 input values and output their sum.

Num         DC 57   ;
Taulu       DS 5    ;
TaulunKoko  EQU 5   ;
Num2        DC 73   ;

Sum         DC 0                    ;
            load r3, =0             ;
            load r1, =0             ;
LOOP        comp  r1, =TaulunKoko   ;
            jnles END               ;
            in    r2, =kbd          ;
            store r2, Taulu(r1)     ;
            add   r3, r2            ;
            add   r1, =1            ;
            jump  LOOP              ;
END         store r3, Sum           ;
            out r3, =crt            ;
            load r3, =Num           ;
            out r3, =crt            ;
            svc sp, =halt           ;
